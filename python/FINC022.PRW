Static Function SaldosInicias(nTipo,dData,cTipoSaldo,cAliasTRB,aPeriodos,aBancos,cNatIni,cNatFin,dDtIni,dDtFin)
Local aArea       := GetArea()
Local cAliasSE8   := GetNextAlias()
Local nSaldoTotal := 0
Local nX          := 0
Local nTrbSaldo   := 0
Local nMoedaBco   := 0
Local cCodIn	  := ""  
Local cAgeIn	  := ""
Local cConIn	  := ""  
Local cAlFIV   := GetNextAlias()

Local nTotPrev	:= 0
Local nSalPrev	:= 0
Local nTotOrc	:= 0
Local nSalOrc	:= 0
Local lVisaoGer := lfluxSint .AND. !Empty(mv_par16)
Local cOrdemVis := ""

Default cNatIni	:= ""
Default cNatFin	:= ""
Default dDtIni	:= dData
Default dDtFin	:= dData

dDtIni	:= dDtIni - 1
dDtFin	:= dDtIni


For nX:=1 to Len(aBancos)
	If nx == 1
		cCodIn += aBancos[nx][2]+"',"
		cAgeIn += aBancos[nx][3]+"',"
		cConIn += aBancos[nx][4]+"',"
	Else
		cCodIn += "'"+aBancos[nx][2]+"'," 	
		cAgeIn += "'"+aBancos[nx][3]+"',"
		cConIn += "'"+aBancos[nx][4]+"'," 	
	EndIf
Next

cCodIn  := "%SA6.A6_COD     IN ('"+SubStr(cCodIn,1,Len(cCodIn)-2)+"') %"
cAgeIn := "%SA6.A6_AGENCIA IN ('"+SubStr(cAgeIn,1,Len(cAgeIn)-2)+"') %"
cConIn := "%SA6.A6_NUMCON  IN ('"+SubStr(cConIn,1,Len(cConIn)-2)+"') %"

BeginSql Alias cAliasSE8
	COLUMN E8_DTSALAT AS DATE

	SELECT SA6.*,E8_FILIAL,E8_DTSALAT,E8_BANCO,E8_AGENCIA,E8_CONTA,E8_SALATUA,E8_SALRECO,E8_SALATUA
	FROM %Table:SA6% SA6,
		%Table:SE8% SE8
	WHERE
		SA6.A6_FILIAL = %xFilial:SA6% AND
		%Exp:cCodIn% AND
		%Exp:cAgeIn% AND
		%Exp:cConIn% AND
		SA6.A6_FLUXCAI IN('S',' ') AND 
		SA6.%NotDel% AND
		SE8.E8_FILIAL = %xFilial:SE8% AND
		SE8.E8_BANCO = SA6.A6_COD AND 
		SE8.E8_AGENCIA = SA6.A6_AGENCIA AND
		SE8.E8_CONTA = SA6.A6_NUMCON AND
		SE8.%NotDel% AND
		SE8.E8_DTSALAT = ( SELECT MAX(TMP.E8_DTSALAT)
							FROM %Table:SE8% TMP
							WHERE 
								TMP.E8_FILIAL = %xFilial:SE8% AND
								TMP.E8_BANCO = SA6.A6_COD AND 
								TMP.E8_AGENCIA = SA6.A6_AGENCIA AND
								TMP.E8_CONTA = SA6.A6_NUMCON AND
								TMP.E8_DTSALAT <= %Exp:dData% AND
								TMP.%NotDel% )
        ORDER BY E8_FILIAL,E8_BANCO,E8_AGENCIA,E8_CONTA,E8_DTSALAT
EndSql
DbSelectArea(cAliasSE8)
While !(cAliasSE8)->(Eof())
	nMoedaBco  := IIf( cPaisLoc="BRA",1,Max((cAliasSE8)->A6_MOEDA,1))	
	If Left(cTipoSaldo,1)=="1" // Somente saldo normal
		nTrbSaldo := xMoeda((cAliasSE8)->E8_SALATUA,nMoedaBco,1)
	ElseIf Left(cTipoSaldo,1)=="2" // Somente os conciliados
		nTrbSaldo := xMoeda((cAliasSE8)->E8_SALRECO,nMoedaBco,1)
	Elseif Left(cTipoSaldo,1)=="3" // Nao conciliados
		nTrbSaldo := xMoeda((cAliasSE8)->E8_SALATUA-(cAliasSE8)->E8_SALRECO,nMoedaBco,1)
	Endif
	nSaldoTotal += nTrbSaldo
	DbSelectArea(cAliasSE8)
	dbSkip()
EndDo

BeginSql Alias cAlFIV
	COLUMN FIV_DATA AS DATE

	SELECT *
	FROM %Table:FIV% FIV
	WHERE
		FIV.FIV_FILIAL = %xFilial:FIV% AND
		FIV.FIV_NATUR >= %Exp:cNatIni% AND
		FIV.FIV_NATUR <= %Exp:cNatFin% AND
		FIV.FIV_DATA <= %Exp:dDtFin% AND
		FIV.%NotDel%
	ORDER BY FIV_DATA, FIV_NATUR
EndSql

DbSelectArea(cAlFIV)
(cAlFIV)->( dbGotop() )
While !(cAlFIV)->(Eof())
	nSalPrev	:= 0
	nSalOrc	:= 0
	nMoedaBco  := IIf( cPaisLoc="BRA",1,Max((cAlFIV)->FIV_MOEDA,1))	
	If (cAlFIV)->FIV_TPSALD == "2"
		If (cAlFIV)->FIV_CARTEI == "P"
			nSalPrev := nSalPrev - xMoeda((cAlFIV)->FIV_VALOR,nMoedaBco,1)
		Else
			nSalPrev := nSalPrev + xMoeda((cAlFIV)->FIV_VALOR,nMoedaBco,1)
		EndIf	
	ElseIf (cAlFIV)->FIV_TPSALD == "1"
		nSalOrc := xMoeda((cAlFIV)->FIV_VALOR,nMoedaBco,1)
	EndIf
	nTotPrev	+= nSalPrev
	nTotOrc		+= nSalOrc
	(cAlFIV)->(dbSkip())
EndDo

DbSelectArea(cAlFIV)
dbCloseArea()
DbSelectArea("FIV")	
	
DbSelectArea(cAliasSE8)
dbCloseArea()
DbSelectArea("SE8")
//-----------------------------------------------------------
// Atualiza o arquivo temporario do fluxo de caixa
//-----------------------------------------------------------
cOrdemVis := Iif( lVisaoGer, Space( __nTamOrdV ), "" )
dbSelectArea(cAliasTRB)
If DbSeek(cOrdemVis+"000"+"Z"+STR0021)
	RecLock(cAliasTRB,.F.)
	(cAliasTRB)->(FieldPut(FieldPos("PREV"+StrZero(1,3)), FieldGet(FieldPos("PREV"+StrZero(1,3))) + nTotPrev))
	(cAliasTRB)->(FieldPut(FieldPos("REAL"+StrZero(1,3)), FieldGet(FieldPos("REAL"+StrZero(1,3))) + nSaldoTotal))
	(cAliasTRB)->(FieldPut(FieldPos("ORC"+StrZero(1,3)), FieldGet(FieldPos("ORC"+StrZero(1,3))) + nTotORC))
Else
	RecLock(cAliasTRB,.T.)
	(cAliasTRB)->SEQ := "000"
	(cAliasTRB)->CART:= "Z"
	(cAliasTRB)->NAT := STR0021 //"Saldos iniciais"
	FieldPut(FieldPos("PREV"+StrZero(1,3)),nTotPrev)
	FieldPut(FieldPos("REAL"+StrZero(1,3)),nSaldoTotal)
	FieldPut(FieldPos("ORC"+StrZero(1,3)),nTotORC)
EndIf

MsUnLock()
RestArea(aArea)
Return Nil

